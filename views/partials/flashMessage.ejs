<% if (locals.errors) { %>
    <% errors.forEach((err)=> {%>
        <div class="alert alert-danger" role="alert">
        <%= err.msg %>
</div>
        <%})%>
        <%}%>

        <% if (locals.message && message!="") { %>  
            <div class="alert alert-success mt-3" role="alert">
            <%= message %>
    </div>
            <%}%>   